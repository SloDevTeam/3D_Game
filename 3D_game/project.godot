; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="3D_Game"
run/main_scene="res://scenes/mainmenu.tscn"

[autoload]

mainmenu_autoload_preload="*res://scripts/mainmenu_autoload_preload.gd"

[compression]

formats/zstd/long_distance_matching=true
formats/zstd/compression_level=22
formats/zstd/window_log_size=10
formats/zlib/compression_level=9
formats/gzip/compression_level=9

[debug]

settings/stdout/print_fps=true
gdscript/completion/autocomplete_setters_and_getters=true

[display]

window/size/height=768
window/size/resizable=false
window/dpi/allow_hidpi=true
window/stretch/aspect="expand"

[editor]

convert_text_resources_to_binary_on_export=true

[editor_plugins]

enabled=PoolStringArray( "decals" )

[gui]

theme/use_hidpi=true

[logging]

file_logging/enable_file_logging=true
file_logging/log_path="res://log.txt"
file_logging/max_log_files=1

[physics]

3d/physics_engine="Bullet"

[rendering]

quality/driver/fallback_to_gles2=true
quality/2d/gles2_use_nvidia_rect_flicker_workaround=true
quality/2d/use_pixel_snap=true
vram_compression/import_bptc=true
vram_compression/import_etc2=false
quality/shadows/filter_mode=2
quality/filters/use_nearest_mipmap_filter=true
quality/subsurface_scattering/quality=0
quality/filters/msaa=2
environment/default_environment="res://Environment.tres"
